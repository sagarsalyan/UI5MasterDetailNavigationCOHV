<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="UnitHelp"
		title="Select Unit"
		class="sapUiPopupWithPadding"
		items="{Unit>/results}"
		liveChange="_handleUnitSearch"
		search="_handleUnitSearch"
		confirm="_handleUnitClose"
		cancel="_handleUnitClose">
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{Unit>Msehi}"
			description="{Unit>Mseht}"/>
	</SelectDialog>
</core:FragmentDefinition>